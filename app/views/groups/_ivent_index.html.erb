<div class="bg-info">
  <div class="col-xs-offset-1 col-xs-10">
    <h4>イベント一覧</h4>
    <% ivents.each do |ivent| %>
      <div class="panel panel-default">
        <div class="panel-heading">
    	    <%= "ID: #{ivent.id} " %><%= link_to ivent.title, ivent %>
        </div>
      	<div class="panel-body">
      		開催予定日: <%= ivent.date %><br>
      		イベント概要: <%= ivent.content %>
      	</div>
      </div>
    <% end %>
  </div>
</div>